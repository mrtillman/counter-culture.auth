@page
@model CounterCulture.Pages.RegisterModel

<form 
    method="POST"
    asp-page="Register"
    asp-page-handler="SubmitRegistration">
  <p>Sign Up</p>
  <input type="email" name="Username" placeholder="Email" value="@Model.TestUser.Username" /><br/>
  <input type="password" name="Password" placeholder="Password" value="@Model.TestUser.Password"/><br/>
  <input type="submit" value="Ok">
</form>